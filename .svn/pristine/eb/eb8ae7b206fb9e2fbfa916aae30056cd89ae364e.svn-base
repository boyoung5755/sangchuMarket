package sangchu.chat.vo;

public class ChatDetailVO {
	/*
		최초 작성자 : 김석호
		최초 작성일 : 2023.08.07 12:39
		
		최근 수정자 : 김석호
		최근 수정일 : 2023.08.10 02:04
		
		수정내역
		2023.08.10 02:04 - 김석호 : 채팅방에서 닉네임을 표시하기 위해 필드 및 게터세터 추가
		
		채팅방 번호에 기록된 채팅 이력을 한 줄씩 저장하는 VO객체
	*/
	/*
		insert into u_chat (chat_no, c_no, chat_date, chat_content, email)
		values(select chat_no where )
	*/
	// 채팅방 번호
	private String chat_no;
	
	// 거래 번호
	private String c_no;
	
	// 작성 일시
	private String chat_date;
	
	// 채팅 내용
	private String chat_content;
	
	// 작성한 사람의 아이디(이메일)
	private String email;

	private String u_nick;
	
	public String getChat_no() {
		return chat_no;
	}

	public void setChat_no(String chat_no) {
		this.chat_no = chat_no;
	}

	public String getC_no() {
		return c_no;
	}

	public void setC_no(String c_no) {
		this.c_no = c_no;
	}

	public String getChat_date() {
		return chat_date;
	}

	public void setChat_date(String chat_date) {
		this.chat_date = chat_date;
	}

	public String getChat_content() {
		return chat_content;
	}

	public void setChat_content(String chat_content) {
		this.chat_content = chat_content;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getU_nick() {
		return u_nick;
	}

	public void setU_nick(String u_nick) {
		this.u_nick = u_nick;
	}
	
	
}
