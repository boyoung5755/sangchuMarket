@font-face {
        font-family: 'GeekbleMalang2WOFF';
        src: url('../font/GeekbleMalang2WOFF.woff') format('woff');
    }


    
    body{
        min-width: 1200px;

    }

    .buttonToLogin {
            justify-content: right;
            width: 1024px;
            display: flex;
        }

        .buttonToLogin a {
            color: rgb(102, 102, 102);
            font-size: 0.8rem;
            margin-bottom: 10px;
        }

        .topDiv {
            margin-top: 10px;
            width: 100%;
            justify-content: center;
            background-color: rgb(238.238, 238);
            display: flex;

        }

    #header {
        margin-top: 30px;
        position: sticky;
        width: 100%;
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
    }

    .logoSection {
        display: inline-flex;
        flex-direction: row;
        text-decoration: none;
        align-items: center;
        margin-right: 80px;
    }

    .font {
        font-family: 'GeekbleMalang2WOFF', sans-serif;
        color: #03c75a;
        font-size: 40px;
        margin-left: 10px;
        margin-top: 13px;
    }

    .searchBoxSection {
        border: 3px solid #03c75a;
        width: 550px;
        height: 40px;
        border-radius: 15px;
        display: inline-flex;
        flex-direction: row;
        justify-items: center;
        align-items: center;
        padding: 0px;
    }

    .search {
        display: flex;
        flex-direction: row;
        justify-items: center;
        align-items: center;
    }

    .search input[type=image]{
        width: 30px;
        height: 30px;
        opacity: 60%;
    }

    .searchBoxInput {
        width: 480px;
        margin-right: 10px;
        margin-left: 10px;
        border: none;
    }

    .mypageNZzim {
        margin-left: 150px;
    }
    .mypageNZzim_img{
        
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-content: center;
        
    }

    .mypageNZzim_img a{
        display: flex;
        align-content: center;
        justify-content: center;
    }

    .mypageNZzim img {
        width: 40px;
        height: 40px;
        opacity: 70%;
        margin-right: 30px;
    }
